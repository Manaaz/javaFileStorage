<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-font-family: serif" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.gb.file.warehouse.PrimaryController">
    <children>
        <Label fx:id="label" text="Файловое хранилище" />
        <Button onAction="#switchToSecondary" text="Авторизация" />
        <Button onAction="#uploadFile" text="Загрузить файл" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
